        <dmRefType>Fault Isolation</dmRefType>
        <dmRefTitle>Fault Isolation Procedure</dmRefTitle>
        <dmRefContent>
          <faultCode>NYCJD04</faultCode>
          <faultDescription>Tire does not function correctly</faultDescription>
          <preliminaryRequirements>
            <supportEquipment>
              <name>Tire pressure gauge</name>
              <manufacturerCode>KZ666</manufacturerCode>
              <partNumber>BSK-TLST-001-01</partNumber>
              <quantity>1 EA</quantity>
              <name>Specialist toolset</name>
              <manufacturerCode>KZ666</manufacturerCode>
              <partNumber>BSK-TLST-001</partNumber>
              <quantity>1 EA</quantity>
            </supportEquipment>
          </preliminaryRequirements>
          <isolationMainProcedure>
            <step>
              <stepNumber>1 (stp-0001)</stepNumber>
              <action>Use the tire pressure gauge to check the pressure.</action>
              <question>What is the tire pressure reading?</question>
              <choices>
                <choice>More than 2700 hPa (Next Action: stp-0002)</choice>
                <choice>Between 100 hPa and 2700 hPa (Next Action: stp-0003)</choice>
                <choice>Less than 100 hPa (Next Action: stp-0004)</choice>
              </choices>
            </step>
            <endStep>Deflate the tire until the pressure is 2700 hPa</endStep>
            <endStep>Inflate the tire as given in DM Ref.</endStep>
            <step>
              <stepNumber>2 (stp-0004)</stepNumber>
              <action>Check the tire for damage.</action>
              <question>Is there damage to the tire?</question>
              <choices>
                <choice>Yes (Next Action: stp-0005)</choice>
                <choice>No (Next Action: stp-0006)</choice>
              </choices>
            </step>
            <endStep>Replace the tire (refer to DM Ref).</endStep>
            <endStep>Replace the inner-tube (refer to DM Ref).</endStep>
          </isolationMainProcedure>
        </dmRefContent>
