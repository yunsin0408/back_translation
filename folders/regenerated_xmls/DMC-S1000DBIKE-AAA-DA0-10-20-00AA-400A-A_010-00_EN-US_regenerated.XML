    </references>
    <faultIsolation>
      <faultIsolationProcedure>
        <fault>NYCJD04</fault>
        <faultDescription>Tire does not function correctly</faultDescription>
        <preliminaryRequirements>
          <requiredConditionGroup>Not applicable</requiredConditionGroup>
          <requiredSupportEquipment>
            <supportEquipment>Tire pressure gauge (KZ666, BSK-TLST-001-01) - 1 EA</supportEquipment>
            <supportEquipment>Specialist toolset (KZ666, BSK-TLST-001) - 1 EA</supportEquipment>
          </requiredSupportEquipment>
          <requiredSupplies>Not applicable</requiredSupplies>
          <requiredSpares>Not applicable</requiredSpares>
          <requiredSafety>Not applicable</requiredSafety>
        </preliminaryRequirements>
        <isolationMainProcedure>
          <step>
            <stepNumber>1</stepNumber>
            <stepDescription>Use the tire pressure gauge to check the pressure.</stepDescription>
            <decision>
              <question>What is the tire pressure reading?</question>
              <answer>
                <condition>More than 2700 hPa</condition>
                <action>Go to Step 2</action>
              </answer>
              <answer>
                <condition>Between 100 hPa and 2700 hPa</condition>
                <action>Go to Step 3</action>
              </answer>
              <answer>
                <condition>Less than 100 hPa</condition>
                <action>Go to Step 4</action>
              </answer>
            </decision>
          </step>
          <step>
            <stepNumber>2</stepNumber>
            <stepDescription>Deflate the tire until the pressure is 2700 hPa.</stepDescription>
          </step>
          <step>
            <stepNumber>3</stepNumber>
            <stepDescription>Inflate the tire as given in DMC-S1000DBIKE-AAA-DA0-10-20-00AA-215-A_010-00_EN-US.</stepDescription>
          </step>
          <step>
            <stepNumber>4</stepNumber>
            <stepDescription>Check the tire for damage.</stepDescription>
            <decision>
              <question>Is there damage to the tire?</question>
              <answer>
                <condition>Yes</condition>
                <action>Go to Step 5</action>
              </answer>
              <answer>
                <condition>No</condition>
                <action>Go to Step 6</action>
              </answer>
            </decision>
          </step>
          <step>
            <stepNumber>5</stepNumber>
            <stepDescription>Replace the tire (refer to DMC-S1000DBIKE-AAA-DA0-10-20-00AA-921-A_010-00_EN-US).</stepDescription>
          </step>
          <step>
            <stepNumber>6</stepNumber>
            <stepDescription>Replace the inner-tube (refer to DMC-S1000DBIKE-AAA-DA0-10-10-00AA-921-A_010-00_EN-US).</stepDescription>
          </step>
        </isolationMainProcedure>
        <closeRequirements>
          <requiredConditionGroup>Not applicable</requiredConditionGroup>
        </closeRequirements>
      </faultIsolationProcedure>
    </faultIsolation>
