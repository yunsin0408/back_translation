                     <identNumber>
                        <manufacturerCode>KZ222</manufacturerCode>
                        <partAndSerialNumber>
                           <partNumber>LL-002</partNumber>
                        </partAndSerialNumber>
                     </identNumber>
                     <reqQuantity>As required</reqQuantity>
                  </supplyDescr>
