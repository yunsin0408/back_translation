                                    <captionEntry colname="col2">
                                       <caption color="co51" captionWidth="30mm">
                                          <captionLine>0 miles</captionLine>
                                       </caption>
                                    </captionEntry>
                                 </captionRow>
                              </captionBody>
                           </captionGroup>
                        </para>
                     </response>
                  </challengeAndResponse>
               </crewDrillStep>
            </crewDrill>
