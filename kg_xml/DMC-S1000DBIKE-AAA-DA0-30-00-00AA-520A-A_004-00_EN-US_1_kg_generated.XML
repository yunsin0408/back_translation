        <WarningCautionParagraph>Be sure to operate on a white surface in order to prevent lost of part that could fall on the floor.</WarningCautionParagraph>
      </Caution>
    </WarningsAndCautions>
    <ProcedureSection>
      <PreliminaryRequirements>
        <NoCondition>No conditions specified.</NoCondition>
        <Personnel>No personnel specified.</Personnel>
        <SupportEquipment>
          <Name>Specialist toolset</Name>
          <Identification>
            <ManufacturerCode>KZ666</ManufacturerCode>
            <PartNumber>BSK-TLST-001</PartNumber>
          </Identification>
          <RequiredQuantity>1 EA</RequiredQuantity>
        </SupportEquipment>
        <Supplies>
          <Supply rdf:about="">
            <ReferenceId>SPEC-0012</ReferenceId>
            <SupplyNumber>BKO200</SupplyNumber>
            <SupplyNumberType>sp01</SupplyNumberType>
            <Name>BIKOIL 200</Name>
            <RequiredQuantity>As Required</RequiredQuantity>
          </Supply>
          <Supply rdf:about="">
            <ReferenceId>SPEC-0013</ReferenceId>
            <SupplyRequirementNumber>EU-OIL3212</SupplyRequirementNumber>
