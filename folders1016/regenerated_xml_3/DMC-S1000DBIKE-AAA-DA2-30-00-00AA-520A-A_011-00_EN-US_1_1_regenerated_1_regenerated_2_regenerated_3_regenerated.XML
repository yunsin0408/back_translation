    <procedure>
      <preliminaryRequirements>
        <reqCondGroup>
          <reqCondNoRef>
            <reqCond>The bicycle is safely held on a work stand</reqCond>
          </reqCondNoRef>
        </reqCondGroup>
        <reqPersons>
          <person>
            <man>A</man>
            <personCategory>Bike Rider</personCategory>
            <personSkill>sk02</personSkill>
            <trade>Operator</trade>
            <estimatedTime>0,5 h</estimatedTime>
          </person>
        </reqPersons>
        <reqSupportEquips>
          <supportEquipDescrGroup>
            <supportEquipDescr>
              <name>Work stand</name>
              <identNumber>
                <manufacturerCode>Bikey</manufacturerCode>
                <partAndSerialNumber>
                  <partNumber>Stand-001</partNumber>
                </partAndSerialNumber>
              </identNumber>
              <reqQuantity>1 EA</reqQuantity>
            </supportEquipDescr>
          </supportEquipDescrGroup>
        </reqSupportEquips>
        <reqSupplies>No Supplies</reqSupplies>
        <reqSpares>No Spares</reqSpares>
        <reqSafety>
          <safetyRqmts>
            <note>
              <notePara>It is not necessary to remove the handlebar for this procedure.</notePara>
            </note>
          </safetyRqmts>
        </reqSafety>
      </preliminaryRequirements>
      <mainProcedure>
        <proceduralStep>Remove the stem (refer to &lt;dmRef&gt; with the DM Ident details mentioned above).</proceduralStep>
        <proceduralStep>Remove:
          <item>the spacers</item>
          <item>the brake cable hangar</item>
          <item>the dust seals</item>
          <item>the conical expansion washer(s) from the steerer tube</item>
        </proceduralStep>
        <proceduralStep>Lift the upper bearing cup off (refer to &lt;internalRef internalRefId="fig-0001" internalRefTargetType="irtt01"/>/) and then remove the fork from the frame.</proceduralStep>
        <figure>
          <id>fig-0001</id>
          <title>Lift the upper bearing cup</title>
          <graphic>infoEntityIdent="ICN-C0419-S1000D0390-001"</graphic>
        </figure>
      </mainProcedure>
    </procedure>
